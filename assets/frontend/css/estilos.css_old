body{
    font-family: 'Roboto', sans-serif;
    background: #FFF
}


h1, h2, h3 ,h4, h5, h6{
    font-family: "venner", Arial, Tahoma, sans-serif;
    font-weight: bold !important;
}

input, textarea, select{
    border-radius: 15px;
}

p {
    font-size: 16px;
    line-height: 18px;
    color: #585858;
    font-family: 'Roboto', sans-serif;
    margin-bottom: 15px;
}
/*
ul, li{
     color: #585858;
}*/

header{
    z-index: 1000;
    position: relative;
}

img{
    max-width: 100%;
}

a{
    color: #0095eb;
    text-decoration: none !important;
    transition: all 0.3s ease-in-out 0s;
}

a:hover{
    text-decoration: none !important;
    transition: all 0.3s ease-in-out 0s;
    color: #bd1f2e;
}
.btn-primary{
    font-family: "venner", Arial, Tahoma, sans-serif;
    background-color: #f2bc00 !important;
    background-image: none !important;
    text-align: center;
    font-size: 24px;
    color: #FFF;
    border-radius: 5px;
    padding: 12px 32px;
    transition: all 0.3s ease-in-out 0s;
}

.btn-primary:hover{
    transition: all 0.3s ease-in-out 0s;
}

.btn-second{
    background: #bd1f2e;
    text-align: center;
    padding: 7px 14px;
    transition: all 0.3s ease-in-out 0s;
    margin-top: 10px;
    position: relative;
    display: inline-block;
    color: #FFF;
    font-weight: bold;
    font-size: 13px;
    margin: 3px 2px;
    border-radius: 6px;
    border:none;
}

.btn-second:hover, .btn-second.active, .btn-second.visited{
    background: #9f1422;
    transition: all 0.3s ease-in-out 0s;
}



.btn-search-social{
    border-radius: 50px;
    height: 30px;
    width: 30px;
    padding: 8px;
    text-align: center; 
}

.reset-total{
    padding:0 !important; 
}

.reset-right{
    padding-right: 0 !important;     
}

.reset-left{
    padding-left: 0 !important;   
}


