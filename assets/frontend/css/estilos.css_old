header{
	border-top: 9px solid #0073d2;
	z-index: 2;
	position: fixed;
    width: 100%;
}

header h1{
	margin: -8px 0 0 21px;
	z-index: 999;
}

header.large {
  height: 120px;
  border-bottom: : 2px solid #099b54;
  background-repeat: repeat-x;
  top: 0;
  background-position-y: 38px;
  background-position-x: -234px;
  transition: all 0.3s ease-in-out 0s;
}

header.large .logotipo {
  width: 80px;
  transition: all 0.3s ease-in-out 0s;
}

header.small {
  height: 107px;
  transition: all 0.3s ease-in-out 0s;
  top: 0;
  background-image: url(../img/line.jpg);
  background-repeat: repeat-x;
  background-position-y: 23px;
  background-position-x: -234px;
}

header.small .logotipo {
    width: 60px;
    transition: all 0.3s ease-in-out 0s;
}

header.small .item-menu2 {
  padding: 10px 0;
}

header.small .social_footer{
  padding: 22px 0;
}

header.small .datos-cabecera li a{
  font-size: 16px
}

header.small .datos-cabecera li a i{
  font-size: 13px;
  padding: 4px;
  width: 22px;
  height: 22px;
}

/***********BANNER********/

.fluid_container {
    bottom: 0;
    left: 0;
    position: relative;
    right: 0;
    margin-bottom: 8px;
    top: 8px;
}
#camera_wrap_4 {
    bottom: 0;
    height: 858px;
    left: 0;
    margin-bottom: 0px !important;
    position: relative;
    right: 0;
    top: 0px;
}
.camera_bar {
    z-index: 2;
}
.camera_thumbs {
    margin-top: -100px;
    position: relative;
    z-index: 1;
}
.camera_thumbs_cont {
    border-radius: 0;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
}
.camera_overlayer {
    opacity: .1;
}

.contenedor-texto {
    width: 100%;
    text-align: center;
    margin: 106px 0 0 0;
    color: #FFF;
    padding: 0;
    height: 753px;
}

hr.style1{
    border-top: 5px solid #FFF;
    margin: 5px 0;
}  

hr.style2{
    border-top: 1px solid #DADADA;
    margin: 5px 0;  
}

hr.style3{
    border-top: 2px dotted #DADADA;
    margin: 5px 0;  
    margin-bottom: 15px;
}

.btn-banner:hover{
    transition: all 0.3s ease-in-out 0s;
}

.btn-banner:hover{
    -webkit-filter: brightness(1.5);
    filter: brightness(1.5);
    transition: all 0.3s ease-in-out 0s;
}

